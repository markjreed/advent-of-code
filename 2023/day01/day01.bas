100 OPEN 1,8,2,"INPUT.SEQ"
110 OPEN 15,8,15:INPUT#15,DS,DS$,T,S:CLOSE 15
120 IF DS THEN PRINT DS$:GOTO 100
130 FOR EF=0 TO 1 STEP 0
140 : LINPUT#1,LI$
150 : EF=(ST AND 64) / 64
160 : F=-1:L=-1
170 : FOR I=1 TO LEN(LI$)
180 :   D$=MID$(LI$,I,1)
190 :   IF D$<"0" OR D$>"9" THEN 230
200 :   D=VAL(D$)
210 :   L=D
220 :   IF F=-1 THEN F=D
230 : NEXT I
240 : T=T+F*10+L
250 NEXT EF
260 CLOSE 1
270 PRINT T
