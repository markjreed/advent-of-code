100 DIM B%(255)
110 DEF FNUW(W) = W - (W < 0) * 65536
120 BLOAD "RANDOMBLOCK.BIN",8,0,POINTER(B%(0))
130 FOR I=0 TO 32
140 : O$ = HEX$(I * 16)
150 : IF LEN(O$)<7 THEN O$=RPT$(48,7-LEN(O$))+O$
160 : PRINT O$;
170 : IF I > 31 THEN 230
180 : FOR J=0 TO 7
190 :   NO$ = HEX$(FNUW(B%(I*8+J))):IF LEN(NO$)=2 THEN NO$="00"+NO$
200 :   HI$ = LEFT$(NO$,2): LO$=RIGHT$(NO$,2)
210 :   PRINT " "LO$HI$;
220 : NEXT J
230 : PRINT
240 NEXT I
250 POWEROFF
